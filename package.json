{
  "name": "typeorm-aurora-data-api-driver",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "main": "dist/typeorm-aurora-data-api-driver.umd.js",
  "module": "dist/typeorm-aurora-data-api-driver.es5.js",
  "typings": "dist/types/typeorm-aurora-data-api-driver.d.ts",
  "files": [
    "dist"
  ],
  "author": "Arseny Yankovsky <contact@arseny.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArsenyYankovsky/typeorm-aurora-data-api-driver"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "tsc --p tsconfig.build.json --module commonjs && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "build:doc": "typedoc --out docs --theme minimal src",
    "tstest": "tsc --version",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:unit": "jest unit",
    "test:mysql-func": "jest mysql-func --runInBand --detectOpenHandles",
    "test:pg-func": "jest pg-func --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": false
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "aws-sdk": "^2.1450.0",
    "colors": "^1.4.0",
    "commitizen": "^4.3.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-config": "^29.6.4",
    "lodash.camelcase": "^4.3.0",
    "prompt": "^1.3.0",
    "replace-in-file": "^7.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "semantic-release": "^21.1.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "data-api-client": "^1.3.0"
  }
}